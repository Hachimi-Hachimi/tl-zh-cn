    "text_block_list": [
        {
            "name": "小栗帽",
            "text": "呼啊……\n呼啊……",
            "choice_data_list": [
                "小栗帽，你是第一名！",
                "小栗帽，你是第一名哦！"
            ]
        },
        {
            "name": "小栗帽",
            "text": "……！"
        },
        {
            "name": "小栗帽",
            "text": "……成功了。\n我……赢了。"
        },
        {
            "name": "玉藻十字",
            "text": "呜～～～～～！！\n呜～呜～输了……！"
        },
        {
            "name": "超级溪流",
            "text": "好乖好乖，别哭了，小玉。\n……我非常了解你的心情哦。"
        },
        {
            "name": "小栗帽",
            "text": "小玉、溪流……"
        },
        {
            "name": "玉藻十字",
            "text": "呜……我才没有哭！\n我有什么办法嘛！"
        },
        {
            "name": "玉藻十字",
            "text": "──谁叫她这么强！\n只是因为我的劲敌……太强了啦！"
        },
        {
            "name": "小栗帽",
            "text": "小玉……"
        },
        {
            "name": "超级溪流",
            "text": "小栗，你跑得很出色。\n你果然……很强。"
        },
        {
            "name": "超级溪流",
            "text": "嗯，恭喜你。\n……恭喜你，小栗！"
        },
        {
            "name": "小栗帽",
            "text": "……有你们两人在。\n我才有办法变得这么强。"
        },
        {
            "name": "小栗帽",
            "text": "真的很谢谢你们。\n我──"
        },
        {
            "name": "欢呼声",
            "text": "（哇啊啊啊啊啊……！！！！）"
        },
        {
            "name": "小栗帽",
            "text": "……！？"
        },
        {
            "name": "欢呼声",
            "text": "小栗帽──！！！！\n恭喜！！"
        },
        {
            "name": "欢呼声",
            "text": "很帅气哦──！"
        },
        {
            "text": "突如其来的欢声雷动。\n这些欢呼声全都朝向了\n当今的霸主小栗帽。"
        },
        {
            "name": "小栗帽",
            "text": "大家……"
        },
        {
            "name": "玉藻十字",
            "text": "快去吧！\n大家都在等你！"
        },
        {
            "name": "小栗帽",
            "text": "……嗯！"
        },
        {
            "name": "小栗帽",
            "text": "……谢谢。"
        },
        {
            "name": "小栗帽",
            "text": "谢谢你们\n替我加油……！"
        },
        {
            "text": "──有这句话就足够了。"
        },
        {
            "text": "这短短的一句话，\n就包含了站上顶点的她\n内心的百感交集……"
        }
    ]
