    "text_block_list": [
        {
            "name": "奇锐骏",
            "text": "准备好了吗？小火山。"
        },
        {
            "name": "北港火山",
            "text": "好了！\n在地偶像的魅力来自活力！\n今天也要麻烦你指导我拳击了！"
        },
        {
            "name": "奇锐骏",
            "text": "很好，那就──"
        },
        {
            "name": "奇锐骏",
            "text": "呼！！喝！！"
        },
        {
            "name": "北港火山",
            "text": "呼！！喝！！"
        },
        {
            "name": "奇锐骏",
            "text": "嘿！！呀啊！！"
        },
        {
            "name": "北港火山",
            "text": "嘿！！呀啊……！！"
        },
        {
            "name": "奇锐骏",
            "text": "欸呀！！嘿呀！！\n哇哒啊啊────────！！！！"
        },
        {
            "name": "北港火山",
            "text": "唔、唔……！"
        },
        {
            "name": "北港火山",
            "text": "呼、呼……呼、呼……"
        },
        {
            "name": "北港火山",
            "text": "奇锐骏，\n你是不是做得比之前更好了啊……？"
        },
        {
            "name": "北港火山",
            "text": "出拳的力道也变得很猛，\n吓了我一跳呢……"
        },
        {
            "name": "奇锐骏",
            "text": "哎呀，有吗？\n我都没发现呢。"
        },
        {
            "name": "奇锐骏",
            "text": "（是吗，真是这样的话……）"
        },
        {
            "name": "奇锐骏",
            "text": "（会是因为训练员\n全力指导我的关系吗？）"
        },
        {
            "name": "奇锐骏",
            "text": "呵呵呵，\n那我应该跟训练员道谢才行呢。"
        }
    ]
}